    "logo": "",
    "keywords": [
        "Telegram",
        "MusicPlayer",
        "VideoPlayer"
    ],
    "repository": "https://github.com/kaal0408/Meow",
    "env": {
        "SESSION": {
            "description": "Pyrogram String Session",
